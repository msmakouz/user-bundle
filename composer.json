{
  "name": "zentlix/user-bundle",
  "type": "symfony-bundle",
  "description": "Zentlix User bundle",
  "license": "proprietary",
  "require": {
    "php": ">=7.4.0",
    "symfony/mailer": "4.4.*",
    "symfony/security-bundle": "4.4.*",
    "odolbeau/phone-number-bundle": "^3.1",
    "zentlix/main-bundle": "^1.4"
  },
  "require-dev": {
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Zentlix\\UserBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test" : "./vendor/bin/simple-phpunit"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "4.4.*"
    }
  }
}
