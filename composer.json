{
  "name": "zentlix/user-bundle",
  "type": "symfony-bundle",
  "description": "Zentlix User bundle",
  "license": "proprietary",
  "require": {
    "php": ">=8.0.2",
    "symfony/mailer": "^5.4 || ^6.0",
    "symfony/security-bundle": "^5.4 || ^6.0",
    "zentlix/main-bundle": "2.0.x-dev"
  },
  "require-dev": {
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Zentlix\\UserBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test" : "./vendor/bin/simple-phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
